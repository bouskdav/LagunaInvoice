@model IEnumerable<LagunaInvoice.Portal.Data.Entities.Contact>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PublicNote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvoicingStreet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvoicingCity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InvoicingZIP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IDNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VATNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WWW)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.R_Company)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.R_DefaultTax)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublicNote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvoicingStreet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvoicingCity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InvoicingZIP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IDNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VATNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WWW)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.R_Company.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.R_DefaultTax.ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
